{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch BookMeAPI",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/BookMeAPI/BookMeAPI.csproj",
    },
    {
      "name": "Launch Next.js",
      "type": "node",
