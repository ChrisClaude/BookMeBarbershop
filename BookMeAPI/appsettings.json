{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedCorsOrigins": ["http://localhost:3000"],
  "ConnectionStrings": {
    "BookMeDb": ""
  },
  "AppSettings": {
    "CacheConfig": {
      "CacheType": "memory",
      "Enabled": true,
      "ConnectionString": "127.0.0.1:6379,ssl=False",
      "SchemaName": "dbo",
      "TableName": "DistributedCache",
      "InstanceName": "bookme",
      "PublishIntervalMs": 500,
      "CacheTime": 10
    },
    "EventConfig": {
      "Server": "localhost:9092",
      "Topic": "bookme"
    },
    "Serilog": {
      "Using": ["Serilog.Sinks.Elasticsearch"],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning"
        }
      },
      "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
      "WriteTo": [
        {
          "Name": "Elasticsearch",
          "Args": {
            "nodeUris": "http://localhost:9200",
            "indexFormat": "myapi-logs-{0:yyyy.MM.dd}",
            "autoRegisterTemplate": true,
            "numberOfShards": 1,
            "numberOfReplicas": 0
          }
        },
        {
          "Name": "Console"
        }
      ],
      "Properties": {
        "Application": "MyAspNetApi"
      },
      "ElasticApiKey": ""
    }
  }
}
