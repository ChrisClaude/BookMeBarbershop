{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BookMeDb": ""
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "QuotaExceededMessage": "You have exceeded the rate limit. Please try again later.",
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 3
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 100
      },
      {
        "Endpoint": "POST:/api/phoneverification/send-code",
        "Period": "1m",
        "Limit": 5
      },
      {
        "Endpoint": "POST:/api/phoneverification/verify-code",
        "Period": "1m",
        "Limit": 10
      }
    ]
  },
  "AppSettings": {
    "AllowedCorsOrigins": ["http://localhost:3000"],
    "CacheConfig": {
      "CacheType": "memory",
      "Enabled": true,
      "CacheTime": 10
    },
    "EventConfig": {
      "Server": "localhost:9092",
      "Topic": "bookme"
    },
    "Elasticsearch": {
      "Uri": "http://localhost:9200",
      "ApiKey": "",
      "Username": "",
      "Password": ""
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning",
          "Microsoft.AspNetCore": "Warning",
          "Microsoft.EntityFrameworkCore": "Warning"
        }
      },
      "Properties": {
        "Application": "BookMeAPI"
      },
      "EnableFileLogging": true
    },
    "OpenTelemetry": {
      "Seq": {
        "LogsUri": "http://localhost:5341/ingest/otlp/v1/logs",
        "TracesUri": "http://localhost:5341/ingest/otlp/v1/traces"
      }
    },
    "TwilioConfig": {},
    "ApplicationInsights": {
      "ConnectionString": ""
    }
  }
}
