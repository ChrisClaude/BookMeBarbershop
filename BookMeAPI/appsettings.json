{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedCorsOrigins": ["http://localhost:3000"],
  "ConnectionStrings": {
    "BookMeDb": ""
  },
  "AppSettings": {
    "CacheConfig": {
      "CacheType": "memory",
      "Enabled": true,
      "ConnectionString": "127.0.0.1:6379,ssl=False",
      "SchemaName": "dbo",
      "TableName": "DistributedCache",
      "InstanceName": "bookme",
      "PublishIntervalMs": 500,
      "CacheTime": 10
    },
    "EventConfig": {
      "Server": "localhost:9092",
      "Topic": "bookme"
    },
    "Elasticsearch": {
      "Uri": "http://localhost:9200",
      "ApiKey": "",
      "Username": "",
      "Password": ""
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning",
          "Microsoft.AspNetCore": "Warning",
          "Microsoft.EntityFrameworkCore": "Warning"
        }
      },
      "Properties": {
        "Application": "BookMeAPI"
      }
    }
  }
}
